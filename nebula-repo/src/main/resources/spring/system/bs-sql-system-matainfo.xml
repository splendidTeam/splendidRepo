<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="MataInfo.findAllMataInfoList" class="loxia.dao.support.DynamicQueryHolder">
		<description></description>
		<constructor-arg>
			<value>
				<![CDATA[  
		       		select * from T_SYS_MATA_INFO 
		       			 where lifecycle !=2
		       		
                 ]]>
			</value>
		</constructor-arg>
	</bean>	
	
	<bean id="MataInfo.findMataInfoByCode" class="loxia.dao.support.DynamicQueryHolder">
		<description></description>
		<constructor-arg>
			<value>
				<![CDATA[  
		       		select * from T_SYS_MATA_INFO 
		       			 where lifecycle !=2
		       			 and code=:code
		       		
                 ]]>
			</value>
		</constructor-arg>
	</bean>	
	
	
	<bean id="MataInfo.findMataInfoListByIds" class="loxia.dao.support.DynamicQueryHolder">
		<description></description>
		<constructor-arg>
			<value>
				<![CDATA[  
		       		select * from T_SYS_MATA_INFO where 
		       		id in (
		       		#foreach( $id in $ids )
    					$id,
					 #end
		       		 -1)
		       		
                 ]]>
			</value>
		</constructor-arg>
	</bean>	
	
	
	<bean id="MataInfo.findMataInfoListByQueryMap" class="loxia.dao.support.DynamicQueryHolder">
		<description></description>
		<constructor-arg>
			<value>
				<![CDATA[  
		       		select * from T_SYS_MATA_INFO where 
		       				lifecycle !=2
		       			
		       				#if($code)
		       					and CODE=:code
		       				#end
		       				
		       				#if($declare)
		       					and DECLARE like '%$declare%'
		       				#end
		       				
		       				#if($value)
		       					and VALUE=:value
		       				#end
		       			
		       				#if($lifecycle)
		       					and LIFECYCLE=:lifecycle
		       				#end
		       				
                 ]]>
			</value>
		</constructor-arg>
	</bean>	
	
	<bean id="MataInfo.findMataInfoListByQueryMapWithPage" class="loxia.dao.support.DynamicQueryHolder">
		<description></description>
		<constructor-arg>
			<value>
				<![CDATA[  
		       		select * from T_SYS_MATA_INFO where 
		       		
		       				lifecycle !=2
		       			
		       				#if($code)
		       					and CODE=:code
		       				#end
		       			
		       				#if($value)
		       					and VALUE like :value
		       				#end
		       				
		       				#if($declare)
		       					and DECLARE like '%$declare%'
		       				#end
		       			
		       				#if($lifecycle)
		       					and LIFECYCLE=:lifecycle
		       				#end
		       				
                 ]]>
			</value>
		</constructor-arg>
	</bean>	
	
	
	<bean id="MataInfo.enableOrDisableMataInfoByIds" class="loxia.dao.support.DynamicQueryHolder">
		<description></description>
		<constructor-arg>
			<value>
				<![CDATA[  
		       		update T_SYS_MATA_INFO
		       		set lifecycle=:state
		       		 where 
		       		id in (
		       		#foreach( $id in $ids )
    					$id,
					 #end
		       		 -1)
		       		 
                 ]]>
			</value>
		</constructor-arg>
	</bean>
	
	<bean id="MataInfo.removeMataInfoByIds" class="loxia.dao.support.DynamicQueryHolder">
		<description></description>
		<constructor-arg>
			<value>
				<![CDATA[  
		       		update T_SYS_MATA_INFO
		       		set lifecycle=2
		       		 where 
		       		id in (
		       		#foreach( $id in $ids )
    					$id,
					 #end
		       		 -1)
		       		 
                 ]]>
			</value>
		</constructor-arg>
	</bean>
	
	<bean id="MataInfo.findAllEffectMataInfoList" class="loxia.dao.support.DynamicQueryHolder">
		<description></description>
		<constructor-arg>
			<value>
				<![CDATA[  
		       		select * from T_SYS_MATA_INFO where lifecycle=1
		       		
                 ]]>
			</value>
		</constructor-arg>
	</bean>
	
		<bean id="MataInfo.findEffectMataInfoListByQueryMap" class="loxia.dao.support.DynamicQueryHolder">
		<description></description>
		<constructor-arg>
			<value>
				<![CDATA[  
		       		select * from T_SYS_MATA_INFO where lifecycle=1
		       			
		       				#if($code)
		       					and CODE=:code
		       				#end
		       			
		       				#if($value)
		       					and VALUE=:value
		       				#end
		       			
		       				#if($lifecycle)
		       					and LIFECYCLE=:lifecycle
		       				#end
		       		
                 ]]>
			</value>
		</constructor-arg>
	</bean>	
	
	<bean id="MataInfo.findEffectMataInfoListByQueryMapWithPage" class="loxia.dao.support.DynamicQueryHolder">
		<description></description>
		<constructor-arg>
			<value>
				<![CDATA[  
		       		select * from T_SYS_MATA_INFO where lifecycle=1
		       			
		       				#if($code)
		       					and CODE=:code
		       				#end
		       			
		       				#if($value)
		       					and VALUE=:value
		       				#end
		       			
		       				#if($lifecycle)
		       					and LIFECYCLE=:lifecycle
		       				#end
                 ]]>
			</value>
		</constructor-arg>
	</bean>	
	
</beans>