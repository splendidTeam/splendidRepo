			<value>
				<![CDATA[  
		       		select * from t_pd_sku_inventory_log where 
		       				1=1
		       				#if($qty)
		       					and QTY=:qty
		       				#end
		       			
		       				#if($extentionCode)
		       					and EXTENTION_CODE=:extentionCode
		       				#end

		       				#if($modifyTime)
		       					and MODIFY_TIME >= :modifyTimeStart
		       				#end
		       				#if($modifyTime)
		       					and MODIFY_TIME <= :modifyTimeEnd
		       				#end

		       				#if($type)
		       					and TYPE=:type
		       				#end
				       		#if($userId)
				       			and USER_ID =:userId
				       		#end
		       				
                 ]]>
			</value>
		</constructor-arg>
	</bean>	
	
	
