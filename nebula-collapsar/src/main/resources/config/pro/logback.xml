<?xml version="1.0" encoding="UTF-8"?>

<!-- scan 当此属性设置为true时，配置文件如果发生改变，将会被重新加载， 默认值为true。 -->
<!-- scanPeriod 设置监测配置文件是否有修改的时间间隔，如果没有给出时间单位，默认单位是毫秒。 当scan为true时，此属性生效。 默认的时间间隔为1分钟。 -->
<!-- debug 当此属性设置为true时，将打印出logback内部日志信息，实时查看logback运行状态。 默认值为false。 -->
<configuration scan="false" scanPeriod="60" debug="false" xmlns="http://logback.com/logback.xsd">

	<!-- 项目名字 -->
	<property name="projectName" value="nebula-ollapsar"></property>
	<!-- 日志文件夹地址 -->
	<property name="logDirectory" value="/home/appuser/logs"></property>

	<!-- 公共log -->
	<appender name="LOG_FILE" class="ch.qos.logback.core.rolling.RollingFileAppender">
		<rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">
			<fileNamePattern>${logDirectory}/${projectName}/${spring.profiles.active}/logFile-%d{yyyyMMdd}.log</fileNamePattern>
			<maxHistory>30</maxHistory>
		</rollingPolicy>
		<filter class="ch.qos.logback.classic.filter.ThresholdFilter">
			<level>info</level>
		</filter>
		<encoder charset="UTF-8" class="ch.qos.logback.classic.encoder.PatternLayoutEncoder">
			<pattern>%d{MMdd HH:mm:ss} %-5level \(%F:%L\) %method\(\) - %m%n</pattern>
		</encoder>
	</appender>

	<!-- 和支付有关的log -->
	<appender name="PAYMENT_LOG_FILE" class="ch.qos.logback.core.rolling.RollingFileAppender">
		<rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">
			<fileNamePattern>${logDirectory}/${projectName}/${spring.profiles.active}/payment/payment-%d{yyyyMMdd}.log</fileNamePattern>
			<maxHistory>90</maxHistory>
		</rollingPolicy>
		<filter class="ch.qos.logback.classic.filter.ThresholdFilter">
			<level>info</level>
		</filter>
		<encoder charset="UTF-8" class="ch.qos.logback.classic.encoder.PatternLayoutEncoder">
			<pattern>%d{HH:mm:ss} %-5level \(%F:%L\) %method\(\) - %m%n</pattern>
		</encoder>
	</appender>

		<!-- **************************nebula************************** -->
	<logger name="com.baozun.nebula.web.filter.UrlDispatcherFilter" level="info" />
	<logger name="com.baozun.store.web.manager.pdp.StdPDPManagerImpl" level="info" />
	<logger name="com.baozun.store.web.tag.ItemBreadCrumbTag" level="info" />
	<!-- <logger name="com.baozun.nebula.tag.ItemBreadCrumbTag" level="info" /> -->

	<!-- **************************feilong****************************************** -->
	<logger name="com.feilong.core.lang.ClassLoaderUtil" level="info" />
	<logger name="com.feilong.core.util.comparator.PropertyComparator" level="info" />
	<logger name="com.feilong.servlet.http.CookieUtil" level="debug" />
	<logger name="com.feilong.servlet.http.RequestUtil" level="info" />
	<logger name="com.feilong.tools.security.symmetric.SymmetricEncryption" level="info" />
	<logger name="com.feilong.spring.web.servlet.interceptor.seo.SeoInterceptor" level="info" />
	<logger name="com.feilong.spring.web.servlet.interceptor.monitor.MonitorInterceptor" level="warn" />
	<logger name="com.feilong.taglib.display.httpconcat.HttpConcatUtil" level="info" />
	<!-- <logger name="com.feilong.taglib.display.breadcrumb.BreadCrumbUtil" level="info" /> -->

	<!-- **************************springframework****************************************** -->
	<logger name="org.springframework" level="DEBUG" />

	<!-- Creating implicit proxy for bean 'memberDao' with 0 common interceptors and 1 specific interceptors -->
